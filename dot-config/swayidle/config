timeout 300 'brightnessctl -s && brightnessctl -q -e1.4 set 10%' resume 'brightnessctl -r'
timeout 600 'dunstctl set-paused true; grim - | magick - -blur 0x05 ~/.cache/lockscreen.png; swaylock' resume 'dunstctl set-paused false'
timeout 720 'systemctl suspend'
before-sleep 'dunstctl set-paused true; grim - | magick - -blur 0x05 ~/.cache/lockscreen.png; swaylock'
after-resume 'dunstctl set-paused false'
